{
    "name": "g8sqh/gpus",
    "description": "GPU server for the BIIGLE image and video annotation tool.",
    "keywords": ["biigle", "image-annotation", "video-annotation", "gpu"],
    "license": "GPL-3.0-only",
    "support": {
        "source": "https://github.com/g8sqh/gpus",
        "issues": "https://github.com/g8sqh/gpus/issues"
    },
    "homepage": "https://biigle.de",
    "authors": [
        {
            "name": "Martin Zurowietz",
            "email": "martin@cebitec.uni-bielefeld.de"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "biigle/laravel-file-cache": "^3.1",
        "biigle/laravel-remote-queue": "^2.0",
        "g8sqh/maia": "dev-master",
        "illuminate/redis": "^5.7",
        "laravel/lumen-framework": "5.7.*",
        "mzur/laravel-openstack-swift": "^1.7",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laravel/tinker": "^1.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": [
            "docker-compose run --rm app php -d memory_limit=1G vendor/bin/phpunit --colors=always"
        ],
        "testf": [
            "docker-compose run --rm app php -d memory_limit=1G vendor/bin/phpunit --colors=always --filter"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "maia": {
            "type": "vcs",
            "url": "https://github.com/g8sqh/maia"
        },
        "largo": {
            "type": "vcs",
            "url": "https://github.com/biigle/largo"
        }
    }
}
